<?xml version='1.0' encoding='ISO-8859-1'?>
<validator>

	<parameter id="app_id" type="integer" required="true" label="Aplicación" maxlen="10" />
	<parameter id="rolename" type="varchar" required="true" label="Nombre del Rol" maxlen="30" />
	<parameter id="description" type="varchar" required="false" label="Descripción" maxlen="80" />

<custom-validator 	
	classname="dinamica.validators.DuplicatedKeyValidator" 
	on-error-label="Este nombre de Rol ya está registrado en la base de datos, use otro nombre."
	sql="nodup.sql" />

</validator>

