<?xml version='1.0' encoding='ISO-8859-1'?>
<config>

	<summary>
		Ver Participantes por Lista de Participantes
	</summary>

	<log>false</log>
	
	<set-request-attribute id="progress" value="file:/WEB-INF/action/progress/progress.htm"/>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>true</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="participantes.sql" source="sql" scope="transaction" on-empty-return="1"/>
		<recordset id="lista.sql" source="sql" scope="transaction" />
	</transaction>

	<output>
		<classname>dinamica.GenericOutput</classname>
		<template>template.htm</template>
		<set-http-headers>true</set-http-headers>
		<content-type>text/html</content-type>
		<expiration>0</expiration>
		<print mode="form" recordset="lista.sql" />
		<print mode="table" recordset="participantes.sql" tag="rows" alternate-colors="true" />
	</output>


	<on-exit return-code="1" forward-to="${def:actionroot}/notfound" />

</config>
