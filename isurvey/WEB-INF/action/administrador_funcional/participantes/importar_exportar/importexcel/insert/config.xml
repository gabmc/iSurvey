<?xml version='1.0' encoding='ISO-8859-1'?>

<config>

	<summary>
		Inserta los registro de un archivo Excel previamente validado
	</summary>
	
	<log>false</log>

	<on-error>${def:actionroot}/error</on-error>

	<transaction>
		<classname>excel.ImportExcel</classname>
		<validator>true</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="participante.sql" source="sql" scope="transaction" />
		<recordset id="int_participante_lista_participantes.sql" source="sql" scope="transaction" />
	</transaction>

	<output>
		<classname>dinamica.GenericOutput</classname>
		<template>template.htm</template>
		<set-http-headers>true</set-http-headers>
		<content-type>text/html</content-type>
		<expiration>0</expiration>
		<print mode="form" recordset="_request"/> 
	</output>

</config>
