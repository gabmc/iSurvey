<!--data grid-->
<table class="grid" width="700px">

	<!--toolbar-->
	<tr>
		<th colspan="14">
			<!--título del grid--> 
			<span style="float: left;">Contenido de la tabla </span>
			<!--panel de control de navegacion--> 
			<span style="float: right;">${req:controls}</span> 
			<!--indicador de progreso-->
			<img id="grid-progress" src="${def:context}/images/progress.gif" style="float: right; margin-right: 10px; display: none"> 
			<!--toolbar del grid-->
			<span style="float: left; margin-left: 40px">
				<img src="${def:context}/images/reload.png" width="18" height="18" onclick="search()" title="Recargar data" class="tool">
				<img src="${def:context}/images/print.png" height="20" width="20" onclick="openPDF()" title="Exportar a PDF" class="tool">
				<img src="${def:context}/images/excel.png" width="18" height="18" onclick="openExcel()" title="Exportar a Excel" class="tool">
			</span>
		</th>
	</tr>

	<!--encabezados-->
	<tr>
		<th colspan="2"></th>
		<th width="8%"><a href="javascript:onclick=sortBy('nombre_lista_participantes_pl0','query-participante.sql')" title="Haga CLICK para ordenar por esta columna">Lista a la que pertenecerá</a></th>
		<th width="8%"><a href="javascript:onclick=sortBy('nombre_participante','query-participante.sql')" title="Haga CLICK para ordenar por esta columna">Nombre</a></th>
		<th width="8%"><a href="javascript:onclick=sortBy('apellido_participante','query-participante.sql')" title="Haga CLICK para ordenar por esta columna">Apellido</a></th>
		<th width="8%"><a href="javascript:onclick=sortBy('cedula_participante','query-participante.sql')" title="Haga CLICK para ordenar por esta columna">Cédula</a></th>
		<th width="8%"><a href="javascript:onclick=sortBy('email_participante','query-participante.sql')" title="Haga CLICK para ordenar por esta columna">e-mail</a></th>
		<th width="8%"><a href="javascript:onclick=sortBy('cargo','query-participante.sql')" title="Haga CLICK para ordenar por esta columna">Cargo</a></th>
		<th width="8%"><a href="javascript:onclick=sortBy('supervisor','query-participante.sql')" title="Haga CLICK para ordenar por esta columna">Supervisor</a></th>
		<th width="8%"><a href="javascript:onclick=sortBy('fecha_nacimiento','query-participante.sql')" title="Haga CLICK para ordenar por esta columna">Fecha de Nacimiento</a></th>
		<th width="8%"><a href="javascript:onclick=sortBy('fecha_ingreso','query-participante.sql')" title="Haga CLICK para ordenar por esta columna">Fecha de Ingreso</a></th>
		<th width="8%"><a href="javascript:onclick=sortBy('sexo','query-participante.sql')" title="Haga CLICK para ordenar por esta columna">Sexo</a></th>
		<th width="8%"><a href="javascript:onclick=sortBy('tipo_nomina','query-participante.sql')" title="Haga CLICK para ordenar por esta columna">Tipo Nómina</a></th>
		<th width="8%"><a href="javascript:onclick=sortBy('funcion','query-participante.sql')" title="Haga CLICK para ordenar por esta columna">Función</a></th>

	</tr>

	<!--data-->
	<rows>
	<tr ${fld:_rowStyle}>
		<td align="center" width="3%"><img src="${def:context}/images/clear.png" width="18" height="18" class="tool" onclick="deleteRecord('${fld:id_participante}')" title="Eliminar"></td>
		<td align="center" width="3%"><img src="${def:context}/images/edit.png" width="18" height="18" style="cursor: pointer" onclick="edit('${fld:id_participante}')" class="tool" title="Editar"></td>
		<td>${fld:nombre_lista_participantes_pl0}</td>
		<td align="left">${fld:nombre_participante}</td>
		<td align="left">${fld:apellido_participante}</td>
		<td align="left">${fld:cedula_participante}</td>
		<td align="left">${fld:email_participante}</td>
		<td align="left">${fld:cargo}</td>
		<td align="left">${fld:supervisor}</td>
		<td align="center">${fld:fecha_nacimiento@dd-MM-yyyy}</td>
		<td align="center">${fld:fecha_ingreso@dd-MM-yyyy}</td>
		<td align="left">${fld:sexo}</td>
		<td align="left">${fld:tipo_nomina}</td>
		<td align="left">${fld:funcion}</td>

	</tr>
	</rows>

</table>